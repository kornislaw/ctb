<h2>Sign in</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => { :class => 'well form-inline'}) do |f| %>
  <%= f.email_field :email, placeholder: 'Email', size: '25'  %>
  <%= f.password_field :password, class: "input-small", placeholder: 'Password' %>
    <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %> <%= f.label :remember_me %>
    <% end -%>
  <%= f.submit "Sign in", class: 'btn' %>
<% end %>


